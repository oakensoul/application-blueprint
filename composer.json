{
    "name": "web-masons/application-blueprint",
    "description": "Cornerstone Application Blueprint",
    "license": "Apache-2.0",
    "keywords": [
        "framework",
        "zf2",
        "oakensoul",
        "skeleton"
    ],
    "homepage": "https://github.com/web-masons/application-blueprint/",
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": ">=2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "web-masons/Cornerstone" : "0.*"
    },
    "minimum-stability": "stable",
    "autoload": {
      "psr-0": {
        "ApplicationBlueprint\\" : "./module/ApplicationBlueprint/src/"
      },
      "classmap": [
        "./module/ApplicationBlueprint/Module.php"
      ]
    },
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/web-masons/Cornerstone"
      },
      {
        "type": "composer",
        "url": "https://packages.zendframework.com/"
      }
    ],
    "scripts" : {
      "post-install-cmd" : [

      ],
      "post-update-cmd" : [

      ]
    }
}
