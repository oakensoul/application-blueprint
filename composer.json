{
    "name": "oakensoul/application-skeleton",
    "description": "Base Application Skeleton",
    "license": "BSD-2-Clause",
    "keywords": [
        "framework",
        "zf2",
        "oakensoul",
        "skeleton"
    ],
    "homepage": "https://github.com/oakensoul/application-skeleton/",
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": ">=2.2",
        "phpunit/phpunit": "3.7.*",
        "oakensoul/Cornerstone" : "0.*"
    },
    "minimum-stability": "stable",
    "autoload": {
      "psr-0": {
        "ApplicationSkeleton\\" : "./module/ApplicationSkeleton/src/"
      },
      "classmap": [
        "./module/ApplicationSkeleton/Module.php"
      ]
    },
    "repositories": [
      {
            "type": "vcs",
            "url": "https://github.com/oakensoul/Cornerstone"
      },
      {
        "type": "composer",
        "url": "https://packages.zendframework.com/"
      }
    ],
    "scripts" : {
      "post-install-cmd" : [
        
      ],
      "post-update-cmd" : [
        
      ]
    }
}
